{
    "name": "SlideCapture.js",
    "short_name": "SlideCapture",
    "description": "A web application for performing homography transformation on camera images.",
    "scope": "./",
    "start_url": "index.html",
    "display": "fullscreen",
    "orientation": "landscape",
    "background_color": "#343a40",
    "theme_color": "#343a40",
    "icons": [
        {
            "src": "./favicons/favicon-192.png",
            "sizes": "192x192",
            "type": "image/png"
        }
    ],
    "permissions": [
        "camera"
    ]
}